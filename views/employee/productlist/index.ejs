<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <fieldset>
        <legend>Product List</legend>
        <table border="1">
            <caption>Product Information</caption>
            <thead>
                <th>ProductId</th>
                <th>Name</th>
                <th>Quantity</th>
                <th>Price</th>
            </thead>
            <tbody>
                <% for(var i=0; i < list.length; i++){ %>
                    <tr>
                      <td><%= list[i].id %></td>
                      <td><%= list[i].name %></td>
                      <td><%= list[i].quantity %></td>
                      <td><%= list[i].price %></td>
                      <td><a a href="/employee/update/<%= list[i].id %>">UPDATE</a> </td>
                      <td><a a href="/employee/delete/<%= list[i].id %>">DELETE</a></td>
                    </tr>
                <% } %>
            </tbody>
        </table><br>
        <a href="/logout">Logout</a>
    </fieldset>
</body>
</html>